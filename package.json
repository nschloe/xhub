    "mini-css-extract-plugin": "^1.6.0",
    "prettier": "^2.3.0",
    "style-loader": "^2.0.0",
    "svg-to-png-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.1.3",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.0",
    "webpack-merge": "^5.7.2"
  },
  "dependencies": {
    "chart.js": "^3.3.2",
