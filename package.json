{
  "name": "xdoc",
  "version": "0.2.4",
  "description": "Extensions for GitHub content",
  "repository": "https://github.com/nschloe/xdoc",
  "scripts": {
    "build": "webpack --config webpack.prod.js && cd dist && zip -r ../xdoc.zip * && cd -",
    "dev": "webpack --config webpack.dev.js",
    "watch": "webpack -w --config webpack.dev.js",
    "format": "prettier --write src *.js",
    "lint": "prettier --check src *.js"
  },
  "author": "Nico Schl√∂mer",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "copy-webpack-plugin": "9.0.1",
    "css-loader": "6.5.1",
    "less": "4.1.2",
    "less-loader": "10.2.0",
    "mini-css-extract-plugin": "2.4.4",
    "prettier": "2.4.1",
    "style-loader": "3.3.1",
    "svg-to-png-loader": "4.0.1",
    "terser-webpack-plugin": "5.2.4",
    "webpack": "5.62.1",
    "webpack-cli": "4.9.1",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {
    "chart.js": "3.6.0",
    "katex": "0.15.1",
    "mermaid": "8.13.3",
    "plotly.js-strict-dist": "2.6.2"
  }
}
