{
  "name": "xdoc",
  "version": "0.2.2",
  "description": "Extensions for GitHub content",
  "repository": "https://github.com/nschloe/xdoc",
  "scripts": {
    "build": "webpack --config webpack.prod.js && cd dist && zip -r ../xdoc.zip * && cd -",
    "dev": "webpack --config webpack.dev.js",
    "watch": "webpack -w --config webpack.dev.js",
    "format": "prettier --write src *.js",
    "lint": "prettier --check src *.js"
  },
  "author": "Nico Schl√∂mer",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "copy-webpack-plugin": "9.0.0",
    "css-loader": "5.2.6",
    "less": "4.1.1",
    "less-loader": "10.0.0",
    "mini-css-extract-plugin": "1.6.1",
    "prettier": "2.3.2",
    "style-loader": "2.0.0",
    "svg-to-png-loader": "4.0.0",
    "terser-webpack-plugin": "5.1.3",
    "webpack": "5.40.0",
    "webpack-cli": "4.7.2",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {
    "chart.js": "3.4.0",
    "katex": "0.13.11",
    "mermaid": "8.11.0",
    "plotly.js-strict-dist": "2.1.0"
  }
}
