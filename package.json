{
  "name": "xhub",
  "version": "0.3.6",
  "description": "Extensions for GitHub content",
  "repository": "https://github.com/nschloe/xhub",
  "scripts": {
    "build": "webpack --config webpack.prod.js && cd dist && zip -r ../xhub.zip * && cd -",
    "dev": "webpack --config webpack.dev.js",
    "watch": "webpack -w --config webpack.dev.js",
    "format": "prettier --write src *.js",
    "lint": "prettier --check src *.js"
  },
  "author": "Nico Schl√∂mer",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "less": "4.1.2",
    "less-loader": "10.2.0",
    "mini-css-extract-plugin": "2.6.0",
    "prettier": "2.6.2",
    "style-loader": "3.3.1",
    "svg-to-png-loader": "4.0.1",
    "terser-webpack-plugin": "5.3.1",
    "webpack": "5.72.1",
    "webpack-cli": "4.9.2",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {
    "chart.js": "3.7.1",
    "chartjs-adapter-date-fns": "2.0.0",
    "katex": "0.15.6",
    "plotly.js-strict-dist": "2.12.1"
  }
}
